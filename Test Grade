test_grade = int(input('How many marks did you get? '))
if test_grade == 70:
    print('You got an A')
elif test_grade in range(60,70):
    print('You got an A')
elif test_grade in range(51,60):
    print('You got a B')
elif test_grade == 50:
    print('You got a C')
elif test_grade in range(40,50):
    print('You got a C')
elif test_grade in range(0,40):
    print('You got an F')
